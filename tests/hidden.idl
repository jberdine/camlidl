/* Test hidden attribute for typedefs and function declarations */

/* Regular public typedef */
typedef int public_type;

/* Hidden typedef - should not appear in .mli */
typedef [hidden] int hidden_type;

/* Regular public function */
int public_func([in] int x);

/* Hidden function - should not appear in .mli */
[hidden]
int hidden_func([in] int x);

/* Hidden with other attributes */
[hidden, blocking]
int hidden_blocking_func([in] int x);

